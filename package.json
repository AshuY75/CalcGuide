{
  "name": "calcguide-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "node scripts/check-imports.js && npm run build:client && npm run build:server && npm run generate && npm run audit",
    "build:client": "vite build --outDir dist/static",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server --emptyOutDir false",
    "generate": "node prerender.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "sitemap": "node generate-sitemap.js",
    "audit": "node scripts/audit-build.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.12.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^16.5.0",
    "vite": "^7.3.1"
  }
}